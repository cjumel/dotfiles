[advice]
    forceDeleteBranch = false
    skippedCherryPicks = false
    statusHints = false
[alias]
    lg = log --oneline --graph --decorate --color
[core]
    autocrlf = input
    editor = nvim
[credential]
    helper = osxkeychain
[init]
    defaultBranch = main
[pull]
    rebase = true
[push]
    default = current
    autoSetupRemote = true
[status]
    showUntrackedFiles = all
[user]
    name = clementjumel
    email = clement.jumel@gmail.com
