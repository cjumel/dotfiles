{
  "title": "Keypad",
  "rules": [
    {
      "description": "Fix keypad period (actual period instead of comma)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "keypad_period",
            "modifiers": {}
          },
          "to": [
            {
              "key_code": "comma",
              "modifiers": ["shift"]
            }
          ]
        }
      ]
    }
  ]
}
