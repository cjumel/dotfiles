# Traditional Jupyter commands when installed with `pip` or similar package managers
alias jp='jupyter'           # Main command
alias jpl='jupyter lab'      # Sub-command to start the Jupyter Lab server
alias jpn='jupyter notebook' # Sub-command to start the Jupyter Notebook server

# Alternate Jupyter commands when Jupyter tools are installed with `pipx`
alias jl='jupyter-lab'      # Sub-command to start the Jupyter Lab server
alias jn='jupyter-notebook' # Sub-command to start the Jupyter Notebook server
